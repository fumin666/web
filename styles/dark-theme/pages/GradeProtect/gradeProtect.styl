.grade-chart-box
  border-radius: 2px;
  background: hsla(0,0%,100%,.05);
  margin-bottom: 20px;
  height: calc((100vh - 60px) / 2.5);
.chart-box-header
  overflow: hidden
  padding: 10px 20px 15px;
  .grade-title
    color: rgba(255, 255, 255, 0.65)
#process-center .processTit {
  height: 40px;
  line-height: 40px;
  font-weight: 600;
}
.Bulletin
    .left_safe
        width: 175px;
        height: 300px;
        background: rgba(0, 0, 0, 0.45);
        border-radius: 2px;
        margin-top 20px;
        text-align left
        .title
            width: 175px;
            height: 41px;
            text-align: center;
            line-height: 41px;
            font-size: 14px;
            color: rgba(255,255,255,.85);
        .left_header
            width: 100%;
            height: 40px;
            text-align: center;
            background: linear-gradient(104.71deg, theme_color_hover 3.04%, theme_iconcolor 95.54%);
            border-radius: 2px;
            line-height 40px
            color theme_fontcolor65
        .left_btn
            border: 1px solid #1991C7;
            box-sizing: border-box;
            border-radius: 22px;
            width: 86px;
            line-height 32px
            height: 32px;
            margin 6px auto
            color #1991C7
            text-align center
            cursor pointer
        .left_btn_yes
            // border: 1px solid theme_color_default;
            background theme_iconcolor
            box-sizing: border-box;
            border-radius: 22px;
            width: 54px;
            line-height 32px
            height: 32px;
            margin 6px auto
            text-align center
            color theme_fontcolor65
            cursor pointer
        .left_content
            cursor pointer
            .left_icon
                line-height 24px
                background: theme_fontcolor65;
                width 24px
                height 24px
                border-radius 50%
                color theme_iconcolor
            .left_iconText
              padding 8px 0px 8px 30px
              background: rgba(0, 0, 0, 0);
              color #fff
              border 0
              i
                color theme_iconcolor
            .iconText1:hover
                color theme_iconcolor
            .iconText2:hover
                color theme_iconcolor
            .iconText3:hover
                color theme_iconcolor
            .iconText4:hover
                color theme_iconcolor
            .power
              color #ccc;
            .power:hover
              color #ccc;
    .details
        margin-top 10px
        background: rgba(0, 0, 0, 0.45);
        .title
            color rgba(255, 255, 255, 0.45);
