.logCenter, logCenterMonitor
  .top-list
    text-align left
    .score-td
      color original_color !important
.logCenter, .logCenterBase, .logCenterDb, .logCenterMonitor, .logCenterOperation, .logCenterProcess
  .s-carousel
    .s-carousel-item.is-active
      background-color mainBgColor
.logCenterBase
  .autoOperation, .autoScriptStatic
    background: #fff
  .ys
    margin: 0 20px 0 10px
    background: #fff
  .logCard
    .box-card
      background: none
  .s-card-diy-body
    height: 100%
  .autoScriptStatic .cardcol .cardview {
    padding: 10px;
    height: 100%;
    border: 1px solid #dcdee2;
    border-radius: 2px;
  }
  .logCenter
    .accountBtns
      li
        background linear-gradient(90.08deg, #FFFFFF -10.96%, #F5F7FA 46.66%, rgba(255, 255, 255, 0) 112.93%);
        &:hover
          background linear-gradient(-89.73deg, #FFFFFF 0.04%, theme_iconcolor 99.97%);
          i
           color: theme_iconcolor
        i
          background: #F5F7FA
          border: 4px solid #F5F7FA;
          border-radius: 50%;
      .active
        background linear-gradient(-89.73deg, #FFFFFF 0.04%, theme_iconcolor 99.97%);
        color: #000
        i
          color theme_iconcolor;
          background: #fff
          border: 4px solid #fff;
          border-radius: 50%;
.logLevelReport
  width: 98%
  .main-content
    .reportName
      padding-bottom: 5px;
      border-bottom: 1px solid #dcdee2;
      margin: 15px;
      font-size: 14px;
      font-weight: 600;
    .s-card
      border: 0px
.log_custom_command{
  .grid{
    display grid;
    border-top:1px solid #F5F7FA;
    grid-template-columns 20% 80%;
    .gridleft{
      border-left:1px solid #F5F7FA;
      border-right:1px solid #F5F7FA;
      border-bottom:1px solid #F5F7FA;
    }
    .gridright{
      border-right:1px solid #F5F7FA;
      border-bottom:1px solid #F5F7FA;
    }
  }
}
