    .deil
      padding 0 15px
      & span
        font-size 14px
        line-height 20px
        color rgba(0,0,0,.65)
      .status
        display inline-block
        width 99%
        font-weight bold
        font-size 12px
      /*.green
        color #15B757*/
      .summary
        display inline-block
        width 99%
        white-space nowrap
        text-overflow ellipsis
        overflow hidden
        vertical-align top
        min-height 20px
        font-size 12px
    .info
      padding 0 15px
      color darkWordColor
      display flex
      align-items: flex-start
      justify-content: space-between
      & p
        white-space nowrap
        text-overflow ellipsis
        overflow hidden
      & span
        flex 1
        font-size 12px
        color rgba(0,0,0,.45)
        line-height 22px
        white-space nowrap
        text-overflow ellipsis
        overflow hidden
      & i
        position relative
        top -2px
    .btns
      border-left: 1px solid #DCDEE2;
      background none
      padding 3px 15px 0 27%
      padding-bottom 2px
      .s-badge + .s-badge
        margin-left 25px
      .badgeCol
        width 44%
        margin 0 auto
      .badgeCol.center
        display block
        margin 0 auto
        width 100%
    .btnCol
      display block
      width: 44px;
      height: 44px;
      margin-right: 4%;
      margin-bottom: 31%;
      border-radius: 50%;
      color: #000;
      background: #fff;
      border: 1px solid #DCDEE2;
      &:hover
        color: theme_iconcolor
        border: 1px solid theme_iconcolor;
      &:active
        color: #fff
        border: 1px solid theme_iconcolor;
        background: theme_iconcolor
      &:last-child
        margin-right 0
        i
          font-size 18px
          opacity 0.65
    .dbbtnCol
      display block
      width: 44px;
      height: 44px;
      margin-right: 4%;
      margin-bottom: 31%;
      border-radius: 50%;
      color: #000;
      background: #f5f7fa;
      border: 1px solid #DCDEE2;
      &:hover
        color: theme_iconcolor
        border: 1px solid theme_iconcolor;
      &:active
        color: #fff
        border: 1px solid theme_iconcolor;
        background: theme_iconcolor
      &:last-child
        margin-right 0
        i
          font-size 18px
          opacity 0.65
    .is-disabled
      color: rgba(0,0,0,.25);
      border: 1px solid #dcdee2;
      background: #f5f5f5;
      cursor: not-allowed;
      opacity: 1;
      &:hover
        color: #939699;
        border: 1px solid #bbc1c8;
        background: #bbc1c8;

